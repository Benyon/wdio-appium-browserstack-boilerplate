{
  "name": "wdio-appium-browserstack-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate to run WebdriverIO tests with Appium to test native applications on iOS and Android on BrowserStack or emulator",
  "author": {
    "name": "Jordan Benyon",
    "email": "jordanbenyon@icloud.com"
  },
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test:app:android": "wdio run ./config/browserstack/wdio.bs.android.conf.ts",
    "test:app:ios": "wdio run ./config/browserstack/wdio.bs.ios.conf.ts",
    "test:app:local:android": "wdio run ./config/emulator/wdio.local.android.conf.ts"
  },
  "repository": "https://github.com/Benyon/wdio-appium-browserstack-boilerplate",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@types/jasmine": "^3.7.4",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "@wdio/appium-service": "^6.12.1",
    "@wdio/browserstack-service": "^6.12.1",
    "@wdio/cli": "^6.12.1",
    "@wdio/jasmine-framework": "^6.11.0",
    "@wdio/local-runner": "^6.12.1",
    "@wdio/spec-reporter": "^6.11.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.28.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-wdio": "^6.6.0",
    "node-fetch": "^2.6.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
